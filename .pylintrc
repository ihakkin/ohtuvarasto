[MAIN]


[DESIGN]
max-statements=15

max-locals=10

[FORMAT]
max-line-length=80


[MESSAGES CONTROL]

disable=raw-checker-failed,
        bad-inline-option,
        locally-disabled,
        file-ignored,
        suppressed-message,
        useless-suppression,
        deprecated-pragma,
        use-symbolic-message-instead,
        missing-module-docstring,
        missing-function-docstring,
        missing-class-docstring


[REFACTORING]
max-nested-blocks=2

